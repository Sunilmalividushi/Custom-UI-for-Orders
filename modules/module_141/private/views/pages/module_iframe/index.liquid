---
slug: module_141/module_iframe/index
response_headers: >
  {
    "X-Frame-Options": null
  }
authorization_policies:
  - modules/siteglide_system/module_is_in_admin
layout: modules/module_141/templates/1
---

{% graphql data = "modules/module_141/properties/get_orders" %}
{{data}}
{% assign  data = data.records.results[0] %}

<h1>All Orders</h1>

<section class="sg-content">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <p>Order ID : {{ data[0].id }}</p>
        <p>Email : {{ data[0].properties['module_field_14/order_2']}}</p>
         <p>Status : {{ data[0].properties['module_field_14/order_3']}}</p>
        <table class="table table-bordered">
          	<thead>
				<th scope="col" class="sg-bg-primary text-white">Order ID</th>
				<th scope="col" class="sg-bg-primary text-white">Email</th>			
				<th scope="col" class="sg-bg-primary text-white">Status</th>
			</thead>
			<tbody>
				{% assign newData = data[0].properties['module_field_custom_14/order_8'] | parse_json %}
			
				{% for item in newData -%}
			
					<tr>
						<td>
							{{ data[0].id }}
						</td>
						<td>
							{{ data[0].properties['module_field_14/order_2']}}
						</td>
						<td>
							{{ data[0].properties['module_field_14/order_3']}}
						</td>
					</tr>
				{% endfor %}

			</tbody>
        </table>
       
      </div>
    </div>
  </div>
</section>